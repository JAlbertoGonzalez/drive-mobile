{
  "expo": {
    "name": "x-cloud-mobile",
    "description": "X Cloud Mobile",
    "slug": "x-cloud-mobile",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00aaff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.internxt.xcloud"
    },
    "android": {
      "package": "com.internxt.xcloud"
    }
  }
}
